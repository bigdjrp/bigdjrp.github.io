{% if about-me_list.first[0] == null %}
    {% for about-me in about-me_list %}
        <li>
            <a href="{{ site.paths.about-me | relative_url }}#{{ about-me | replace:' ','-' }}" title="{{ about-me | join: "/" }}">
            {{ about-me | join: "/" }} <span>{{ site.about-me[about-me].size }}</span></a>
        </li>
    {% endfor %}
{% else %}
    {% for about-me in about-me_list %}
        <li>
            <a href="{{ site.paths.about-me | relative_url }}#{{ about-me[0] | replace:' ','-' }}" title="{{ about-me[0] | join: "/" }}">
            {{ about-me[0] | join: "/" }} <span>{{ about-me[1].size }}</span></a>
        </li>
    {% endfor %}
{% endif %}

{% assign about-me_list = nil %}
